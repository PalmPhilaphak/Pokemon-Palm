{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "types": ["@nuxt/types"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
